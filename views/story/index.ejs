<%- include('../partials/header.ejs')%>

<div class="content">
  <% if(stories.length){ %>
  <h3>Select a story to read</h3>
  <% stories.forEach(story=> {%>
  <p>
    <a href="/stories/<%=story.id%>">
      <%= story.title %>
    </a>
  </p>
  <% }); } else {%>
  <p>There are no stories to display.</p>
  <%}%>
    </div>
    
<%- include('../partials/footer.ejs')%>